@model Vaccination.ViewModels.VaccineIndexViewModel

@{
    ViewData["Title"] = "Vaccines";
}



<table class="table table-hover">
    <thead>
    <tr>
        <th scope="col">Namn</th>
        <th scope="col">Supplier</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var vaccine in Model.Vaccines)
    {
        <tr>
            <td>@vaccine.Name</td>
            <td>@vaccine.Supplier</td>
            <td><a asp-controller="Vaccine" asp-action="Edit" asp-route-Id="@vaccine.Id" class="btn btn-secondary btn-lg active" role="button" aria-pressed="true">Edit</a></td>
        </tr>
    }
        
    <form method="get" asp-controller="Vaccine" asp-action="Index">

    </form>
    </tbody>
</table>